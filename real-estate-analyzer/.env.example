# PostgreSQL Database URL
# Example: postgresql://user:password@hostname:port/database_name
DATABASE_URL=postgresql://user:pass@localhost/realestate

# API Rate Limiting (requests per minute, or a similar interval)
API_RATE_LIMIT=100

# Cache Time-To-Live (seconds)
CACHE_TTL=3600

# Logging Level (e.g., DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Real Estate Analysis Parameters
ANALYSIS_RADIUS=0.5 # Default radius in miles for finding comparable properties
MAX_PROPERTY_AGE=6  # Default maximum age in months for comparable sales data
